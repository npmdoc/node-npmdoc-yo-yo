{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "yo-yo",
    "description": "api documentation for [yo-yo (v1.4.0)](https://github.com/maxogden/yo-yo#readme) [![npm package](https://img.shields.io/npm/v/npmdoc-yo-yo.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-yo-yo) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-yo-yo.svg)](https://travis-ci.org/npmdoc/node-npmdoc-yo-yo)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "utility2": "kaizhu256/node-utility2#alpha",
        "yo-yo": "*"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-yo-yo",
    "keywords": [
        "documentation",
        "yo-yo"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_yo_yo.js",
    "name": "npmdoc-yo-yo",
    "nameAlias": "npmdoc_yo_yo",
    "nameOriginal": "npmdoc-yo-yo",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-yo-yo"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.2"
}
